{
    "manifest_version": 3,
    "name": "Image Diagnosis",
    "description": "'Image Diagnosis' extension uses ‘OpenAI’ technology to give a general diagnosis based on an image. ",
    "version": "1.1.0",
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
    "background":[{
      "service_worker": "ads.js"
      }],
        "action": {
            "default_icon": "icon.png",
            "default_popup": "mynotes.html"
        },
       "permissions": ["tts"]

}
